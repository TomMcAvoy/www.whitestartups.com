{
  "compilerOptions": {
    "baseUrl": ".",
    "esModuleInterop": true,
    "target": "es6",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "strictNullChecks": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "incremental": true,
    "jsx": "preserve", // Ensure this is set to "react" or "react-jsx"
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "sourceMap": true,
    "isolatedModules": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@payload-config": [
        "./src/payload.config.ts"
      ],
      "react": [
        "./node_modules/@types/react"
      ],
      "@/*": [
        "./src/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@custom_components/*": [
        "src/components/Custom_components/*"
      ], // Ensure this line is present
      "@custom_pages/*": [
        "src/custom_pages/*"
      ],
      "@styles/*": [
        "src/styles/*"
      ],
      "@workers/*": [
        "src/workers/*"
      ],
      "@utils/*": [
        "src/utils/*"
      ],
      "@hooks/*": [
        "src/hooks/*"
      ],
      "@contexts/*": [
        "src/contexts/*"
      ],
      "@services/*": [
        "src/services/*"
      ],
      "@assets/*": [
        "src/assets/*"
      ],
      "@providers/*": [
        "src/providers/*"
      ],
      "@endpoints/*": [
        "src/endpoints/*"
      ],
      "@plugins/*": [
        "src/plugins/*"
      ],
      "@access/*": [
        "src/access/*"
      ],
      "@fields/*": [
        "src/fields/*"
      ],
      "@blocks/*": [
        "src/blocks/*"
      ],
      "@collections/*": [
        "src/collections/*"
      ],
      "@search/*": [
        "src/search/*"
      ],
      "@app/*": [
        "src/app/*"
      ],
      "@heros/*": [
        "src/heros/*"
      ],
      "@middleware/*": [
        "src/middleware/*"
      ],
      "@localtypes/*": ["src/types/*"],
    }
  },
  "include": [
    "src",
    "types",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "redirects.js",
    "next.config.js"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}
